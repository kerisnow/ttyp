<?xml version="1.0" encoding="utf-8"?>
<GridLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:rowCount="2"
    android:columnCount="1"
    android:background="@color/whtie"
    >
    <!--第一行-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_row="0"
        android:layout_column="0"
        >
        <com.ttyp.tiantao.assembly.myButton.MenuButton
            android:id="@+id/youx_menu_bar_button1"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:button_image_src="@drawable/logo"
            app:button_text="品牌名"
            app:button_text_color="@color/black"
            android:padding="10dp"
         />
        <com.ttyp.tiantao.assembly.myButton.MenuButton
            android:id="@+id/youx_menu_bar_button2"
